    " \n",
    "small=min(N,M)\n",
    "big  =max(N,M)\n",
    "if small==1:\n",
    "    if big==1:\n",
    "        print (1)\n",
    "    elif big==2:\n",
    "        print (0)\n",
    "    else:\n",
    "        print (big-2)\n",
    " \n",
    "elif small==2:\n",
    "    print (0)\n",
    "    \n",
    "else:\n",
    "    print ((small-2)*(big-2))\n"
   ]
