   "source": [
    "N = int(input())\n",
    "D,X =  map(int, input().split())\n",
    "N_chocolate=0\n",
    "for i in range (N):\n",
    "\tA= int(input())\n",
    "\tif A == 1:\n",
    "\t\tN_chocolate=N_chocolate+D-1\n",
    "\telse:\n",
    "\t\tN_chocolate=N_chocolate+int((D-1)/A)\n",
    "output=N_chocolate+X+N\n",
    "print (output)"
   ]
  }
