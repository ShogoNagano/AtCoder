   "source": [
    "N, M, X = map(int, input().split())\n",
    "A=list(map(int, input().split()))\n",
    "\n",
    "N_more_count =0\n",
    "N_less_count =0\n",
    "\n",
    "for i in range (M):\n",
    "    AM = int(A[i])\n",
    "    if AM>X:\n",
    "        N_more_count=N_more_count+1\n",
    "    if AM<X:\n",
    "        N_less_count=N_less_count+1\n",
    "print(min(N_less_count,N_more_count))\n"
   ]
  }
